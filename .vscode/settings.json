{
  "MicroPython.executeButton": [
    {
      "alignment": "left",
      "command": "extension.executeFile",
      "priority": 3.5,
      "text": "▶",
      "tooltip": "运行"
    }
  ],
  "MicroPython.syncButton": [
    {
      "alignment": "left",
      "command": "extension.execute",
      "priority": 4,
      "text": "$(sync)",
      "tooltip": "同步"
    }
  ],
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  },
  "deno.enablePaths": [
    "supabase/functions"
  ],
  "deno.lint": true,
  "deno.unstable": true
}
